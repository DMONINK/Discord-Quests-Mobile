{
  "manifest_version": 2,
  "name": "Discord Kiwi Runner",
  "version": "1.0",
  "description": "Adds a Run Code button for Discord script on Kiwi browser",
  "permissions": [
    "https://discord.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://discord.com/*"],
      "js": ["content.js"],
      "css": ["button.css"],
      "run_at": "document_idle"
    }
  ]
}
